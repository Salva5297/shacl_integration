inc-ex:ddbe1d39-0dd7-4f34-a386-f23ae54e64d3 a inc:Inconsistency ;
    inc:hasInconsistencyAxiom sh:or ;
    inc:hasInconsistencyInClass ssn:System,
        <https://saref.etsi.org/saref4syst/System> ;
    inc:hasReport "Logical constraints not compatible for intersection operation." .

inc-ex:f95cde4f-43d8-4f46-94e0-0691e93dac77 a inc:Inconsistency ;
    inc:hasInconsistencyAxiom sh:maxCount,
        sh:minCount ;
